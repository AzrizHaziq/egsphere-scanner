{
  "presets": ["next/babel"],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@Assets": "./assets",
          "@Components": "./components",
          "@Helpers": "./helpers",
          "@Layout": "./layout",
          "@Pages": "./pages"
        }
      }
    ]
  ]
}
